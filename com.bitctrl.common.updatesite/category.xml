<?xml version="1.0" encoding="UTF-8"?>
<site>


	<category-def name="support_plugins" label="Support-Plugins" />
	<iu>
		<category name="support_plugins" />
		<query>
			<expression type="match">providedCapabilities.exists(p |
				p.namespace == 'osgi.bundle')</expression>
		</query>
	</iu>

	<category-def name="external_support" label="Externe Bibliotheken" />
	<feature id="com.bitctrl.jackson.feature" version="0.0.0">
		<category name="external_support" />
	</feature>
	<feature id="com.bitctrl.jersey.feature" version="0.0.0">
		<category name="external_support" />
	</feature>

</site>
